package oopIntro;

public class Product {

	// Constructor yapýcý bloktur. Özellik tutucu iþ yapýcý class.

	int id;
	String name;
	double unitPrice;
	String detail;
	double discount;

	public Product() {

	}

	public Product(int id, String name, double unitPrice, String detail,double discount) {
		super();
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
		this.discount = discount;
	}

}
