package Entities;

import Abstract.Entity;

public class Campaign implements Entity {

	private int id;
	private String campaingName;
	private String campaingDiscountRate;

	public Campaign(int id, String campaingName, String campaingDiscountRate) {
		
		this.id = id;
		this.campaingName = campaingName;
		this.campaingDiscountRate = campaingDiscountRate;
		
	}

	public int getId() {
		
		return id;
	}

	public void setId(int id) {
		
		this.id = id;
	}

	public String getCampaingName() {
		
		return campaingName;
	}

	public void setCampaingName(String campaingName) {
		
		this.campaingName = campaingName;
	}

	public String getCampaingDiscountRate() {
		
		return campaingDiscountRate;
	}

	public void setCampaingDiscountRate(String campaingDiscountRate) {
		
		this.campaingDiscountRate = campaingDiscountRate;
	}

}
